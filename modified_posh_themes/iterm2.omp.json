{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "final_space": true,
  "console_title": true,
  "console_title_style": "template",
  "console_title_template": "{{ .Host }} in {{ .Folder }}",
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "vertical_offset": 1,
      "segments": [
        {
          "type": "os",
          "style": "powerline",
          "powerline_symbol": "\uE0B0",
          "foreground": "#86ff15",
          "background": "#F70361",
          "properties": {
            "prefix": " ",
            "postfix": ""
          }
        },
        {
          "type": "session",
          "style": "powerline",
          "powerline_symbol": "\uE0B0",
          "foreground": "#021527",
          "background": "#e97406",
          "properties": {
            "prefix": "",
            "display_host": false,
            "postfix": " "
          }
        },
        {
          "type": "path",
          "style": "powerline",
          "powerline_symbol": "\uE0B0",
          "foreground": "#fec303",
          "background": "#e7035a",
          "properties": {
            "prefix": " \uE5FF ",
            "style": "full",
            "folder_separator_icon": "\\"
          }
        },
        {
          "type": "git",
          "style": "powerline",
          "powerline_symbol": "î‚°",
          "foreground": "#efff0d",
          "background": "#820492",
          "properties": {
            "display_stash_count": true,
            "display_upstream_icon": true,
            "status_colors_enabled": true,
            "display_status": true,
            "local_changes_color": "#011A2E",
            "ahead_and_behind_color": "#f26d50",
            "behind_color": "#f17c37",
            "ahead_color": "#89d1dc",
            "stash_count_icon": "\uF692 "
          }
        },
        {
          "type": "exit",
          "style": "powerline",
          "powerline_symbol": "î‚°",
          "invert_powerline": false,
          "foreground": "#ffffff",
          "background": "#2e9599",
          "leading_diamond": "",
          "trailing_diamond": "î‚´",
          "properties": {
            "always_enabled": true,
            "color_background": true,
            "display_exit_code": false,
            "error_color": "#c70230",
            "prefix": " îˆº"
          }
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "right",
      "segments": [
        {
          "type": "battery",
          "style": "powerline",
          "invert_powerline": true,
          "powerline_symbol": "\uE0B2",
          "foreground": "#fff",
          "background": "#f36943",
          "properties": {
            "battery_icon": "\f583",
            "color_background": true,
            "charged_color": "#26ff09",
            "charging_color": "#f17501",
            "discharging_color": "#01121F",
            "postfix": "\uF295 \uf583 "
          }
        },
        {
          "type": "dotnet",
          "style": "powerline",
          "powerline_symbol": "î‚°",
          "invert_powerline": false,
          "foreground": "#f6e8ff",
          "background": "#024478",
          "leading_diamond": "",
          "trailing_diamond": "",
          "properties": {
            "display_version": true,
            "prefix": " îœŒ "
          }
        },
        {
          "type": "python",
          "style": "powerline",
          "invert_powerline": true,
          "powerline_symbol": "\uE0B2",
          "foreground": "#003544",
          "background": "#0087D8",
          "properties": {
            "prefix": " \uE235 ",
            "display_version": false,
            "display_mode": "context",
            "display_virtual_env": true
          }
        },
        {
          "type": "time",
          "style": "powerline",
          "invert_powerline": true,
          "powerline_symbol": "\uE0B2",
          "foreground": "#ffbe0b",
          "background": "#F70361",
          "properties": {
            "prefix": "<#fff> \uf64f </>"
          }
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "type": "root",
          "style": "plain",
          "foreground": "#FFD700",
          "properties": {
            "root_icon": "âš¡"
          }
        },
        {
          "type": "text",
          "style": "plain",
          "foreground": "#f1184c",
          "properties": {
            "prefix": "",
            "text": "ðŸš€"
          }
        }
      ]
    }
  ]
}
