{
    "$schema": "https://aka.ms/terminal-profiles-schema",
    "actions": 
    [
        {
            "command": 
            {
                "action": "copy",
                "singleLine": false
            },
            "keys": "ctrl+c"
        },
        {
            "command": "unbound",
            "keys": "ctrl+x"
        },
        {
            "command": "find",
            "keys": "ctrl+f"
        },
        {
            "command": "paste",
            "keys": "ctrl+v"
        },
        {
